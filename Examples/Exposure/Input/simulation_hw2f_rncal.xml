<?xml version="1.0"?>
<Simulation>
    <Parameters>
      <Grid>5000,1D</Grid>
      <Calendar>EUR</Calendar>
      <Sequence>MersenneTwister</Sequence>
      <Scenario>Simple</Scenario>
      <Seed>42</Seed>
      <Samples>1</Samples>
      <Ordering>Steps</Ordering>
      <DirectionIntegers>JoeKuoD7</DirectionIntegers>
    </Parameters>
  <CrossAssetModel>
    <DomesticCcy>EUR</DomesticCcy>
    <Currencies>
      <Currency>EUR</Currency>
    </Currencies>
    <BootstrapTolerance>0.0001</BootstrapTolerance>
    <Discretization>Euler</Discretization>
    <Measure>BA</Measure>
    <InterestRateModels>
      <HWModel ccy="default">
        <CalibrationType>StatisticalWithRiskNeutralVolatility</CalibrationType>
        <Reversion>
          <Calibrate>N</Calibrate>
          <ParamType>Constant</ParamType>
          <TimeGrid/>
          <InitialValue>
            <Kappa>-0.000000048673,-0.24532070948,-0.056427887126,0.510590372873</Kappa>
          </InitialValue>
        </Reversion>
        <PCALoadings>
          <Loadings>0.353553391,0.0</Loadings>
          <Loadings>0.235537396,-0.99634127</Loadings>
        </PCALoadings>
        <PCASigma0>
          <Calibrate>Y</Calibrate>
          <ParamType>Piecewise</ParamType>
          <TimeGrid/>
          <InitialValue>0.01</InitialValue>
        </PCASigma0>
        <PCASigmaRatios>1.0, 0.428571429</PCASigmaRatios>
        <CalibrationSwaptions>
          <Expiries> 1Y,  2Y,  4Y,  6Y,  8Y, 10Y, 12Y, 14Y, 16Y, 18Y, 19Y</Expiries>
          <Terms>   19Y, 18Y, 16Y, 14Y, 12Y, 10Y,  8Y,  6Y,  4Y,  2Y,  1Y</Terms>
          <Strikes/>
        </CalibrationSwaptions>
      </HWModel>
    </InterestRateModels>
    <ForeignExchangeModels>
      <CrossCcyLGM foreignCcy="default">
        <DomesticCcy>EUR</DomesticCcy>
        <CalibrationType>None</CalibrationType>
        <Sigma>
          <Calibrate>N</Calibrate>
          <ParamType>Constant</ParamType>
          <TimeGrid/>
          <InitialValue>0.1</InitialValue>
        </Sigma>
        <CalibrationOptions/>
      </CrossCcyLGM>
    </ForeignExchangeModels>
    <InstantaneousCorrelations/>
  </CrossAssetModel>
  <Market>
    <BaseCurrency>EUR</BaseCurrency>
    <Currencies>
      <Currency>EUR</Currency>
    </Currencies>
    <YieldCurves>
      <Configuration>
        <Tenors>1Y,2Y,3Y,5Y,10Y,15Y,20Y,30Y</Tenors>
        <Interpolation>LogLinear</Interpolation>
        <Extrapolation>Y</Extrapolation>
      </Configuration>
    </YieldCurves>
    <Indices/>
  </Market>
</Simulation>
