      <xs:element type="xs:string" name="YYInflationIndexCurvesId" minOccurs="0"/>
      <xs:element type="xs:string" name="FxSpotsId" minOccurs="0"/>
      <xs:element type="xs:string" name="BaseCorrelationsId" minOccurs="0"/>
