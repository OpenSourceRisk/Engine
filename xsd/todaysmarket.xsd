      <xs:element type="xs:string" name="CommodityVolatilitiesId" minOccurs="0"/>
      <xs:element type="xs:string" name="CorrelationsId" minOccurs="0"/>
    </xs:all>
    <xs:attribute type="xs:string" name="id" use="required"/>
  </xs:complexType>

