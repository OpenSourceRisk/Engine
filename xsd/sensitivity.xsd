      <xs:element type="xs:string" name="ShiftTenors"/>
      <xs:element type="parconversion" name="ParConversion" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute type="xs:string" name="name" use="required"/>
  </xs:complexType>


  <xs:complexType name="discountcurve">
