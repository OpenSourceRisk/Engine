    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="recoveryrates">
    <xs:sequence>
      <xs:element type="recoveryrate" name="RecoverRate" maxOccurs="unbounded" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="fxvolatilities">
    <xs:sequence>
